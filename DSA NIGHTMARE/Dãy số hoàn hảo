//DÃ¹ng set
#include <bits/stdc++.h>

using namespace std;

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	cout.tie(0);
	int n, k;
	cin >> n >> k;
	set<int> se;
	for (int i = 0; i < n; i++) {
		int x;
		cin >> x;
		se.insert(x);
	}
	if (se.size() == k) {
		cout << "Perfect";
	} else {
		cout << "Bad";
	}
	return 0;
}
