//DÃ¹ng map
#include <bits/stdc++.h>

using namespace std;

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	cout.tie(0);
	int n;
	cin >> n;
	map<int, int> mp;
	for (int i = 0; i < n; i++) {
		int x;
		cin >> x;
		mp[x]++;
	}
	int fre = INT_MIN;
	for (auto it : mp) {
		if (it.second > fre) {
			fre = it.second;
		}
	}
	cout << fre << endl;
	for (auto it : mp) {
		cout << it.first << ' ';
	}
	return 0;
}
