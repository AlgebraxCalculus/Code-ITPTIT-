#include <stdio.h>

	int main() {
	int n;
	scanf("%d",&n);
	for (int i = 1; i <= n; i++) {
	if (i < n) {
	for (int j = i;j >= 1; j--) {
		printf("%d", j);
	}
	for (int j = 2; j <= n - i + 1; j++) {
			printf("%d",j);
	}
	} else { 
		for (int j = i; j >= i - n + 1; j--)
		{
			printf("%d", j);
		}
	}
	printf("\n");
	}
	return 0;
}
