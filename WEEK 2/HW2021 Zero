#include <stdio.h>

int main() {
	int t;
	scanf("%d", &t);
	while (t--) {
		int n;
    	scanf("%d", &n);
    	int a[n + 5];
		for (int i = 0; i < n; i++) {
    	scanf("%d", &a[i]);
    	}
    	for (int i = 0; i < n; i++) {
    		if (a[i] < 0) {
    			a[i] = 0;
    		}
    	}
    	for (int i = 0; i < n; i++) {
    		printf("%d ", a[i]);
    		}
    	printf("\n");
    	}
	return 0;
}
