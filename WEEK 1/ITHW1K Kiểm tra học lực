#include <stdio.h>
// IT Club PTIT Hanoi

int main() {
	// Your code goes here
	int toan, van, anh, diemtrungbinh;
    scanf("%d %d %d", &toan, &van, &anh);
    diemtrungbinh = (toan + van + anh) / 3;
    if (diemtrungbinh < 5) {
        printf("yeu");
    } else if (diemtrungbinh >=5 && diemtrungbinh < 6.5) {
        printf("trung binh");
    } else if (diemtrungbinh >= 6.5 && diemtrungbinh < 8) {
        printf("kha");
    } else {
        printf("gioi");
    }
	return 0;
}
