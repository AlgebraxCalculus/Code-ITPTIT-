#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    int a[n + 5][n + 5];
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%d", &a[i][j]);
        }
    }
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            printf("%d ", a[n - 1 - j][i]);
        }
        printf("\n");
    }
    return 0;
}
