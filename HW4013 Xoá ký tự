#include <stdio.h>
#include <string.h>

int main() {
    int t; 
    scanf("%d", &t);
    while (t--) {
        scanf("\n"); 
        char s[1001];
        gets(s);
        char a;
        scanf("%c", &a);
        for (int i = 0; i < strlen(s); i++) {
          if (s[i] != a) {
            printf("%c", s[i]);
          }
        }
        printf("\n");
    }
    return 0;
}
