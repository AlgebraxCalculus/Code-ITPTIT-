#include <stdio.h>
// IT Club PTIT Hanoi

int main() {
	// Your code goes here
	int a, b, c, d, solonnhat;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    if (a > b && a > c && a > d) {
        solonnhat = a;
    } else if (b > c && b > d) {
        solonnhat = b;
    } else if (c > d) {
        solonnhat = c;
    } else {
        solonnhat = d;
    } 
    printf("%d", solonnhat);
	return 0;
}
