#include <bits/stdc++.h>

using namespace std;

int main() {
	ios_base::sync_with_stdio(0); 
	cin.tie(0); 
	cout.tie(0);
	int t;
	cin >> t;
	while (t--) {
		string s;
		cin >> s;
		int max_num = 0, so = 0;
		for (int i = 0; i < s.size(); i++) {
			if (s[i] >= '0' && s[i] <= '9') {
				so = so * 10 + (s[i] - '0');
			} else {
				max_num = max(max_num, so);
				so = 0;
			}
		}
		cout << max_num << endl;
	}
	return 0;
}
